<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kobylkin.musicstoresvrn"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.kobylkin.musicstoresvrn.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.kobylkin.musicstoresvrn.ViewStore"
            android:label="@string/title_activity_view_store" >
        </activity>
        <activity
            android:name="com.kobylkin.musicstoresvrn.ViewInstruments"
            android:label="@string/title_activity_view_instruments" >
        </activity>
        <activity
            android:name="com.kobylkin.musicstoresvrn.InstrumentItem"
            android:label="@string/title_activity_instrument_item" >
        </activity>
    </application>

</manifest>
